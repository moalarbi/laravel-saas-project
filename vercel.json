{
    "version": 2,
    "framework": "laravel",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.5"
        }
    },
    "routes": [
        {
            "src": "/(css|js|images|favicon.ico|robots.txt)(.*)",
            "dest": "/public/$1$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "LOG_CHANNEL": "stderr",
        "DB_CONNECTION": "sqlite",
        "DB_DATABASE": "/tmp/database.sqlite"
    }
}
